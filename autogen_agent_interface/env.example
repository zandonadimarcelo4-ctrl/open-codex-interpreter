# Environment Variables
# Copie este arquivo para .env e configure as variáveis

# Node Environment
NODE_ENV=development
PORT=3000

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
# Modelo que suporta function calling (tools) - RECOMENDADO
# Qwen2.5:14b é muito inteligente, bom em código, e suporta tools
DEFAULT_MODEL=qwen2.5:14b
# Alternativas: qwen2.5:32b (mais inteligente, maior), qwen2.5:7b (mais rápido, menor), llama3.2:3b (menor, suporta tools)

# Ollama Timeout Configuration (em milissegundos)
# Timeout padrão para todas as requisições
OLLAMA_TIMEOUT_MS=120000
# Timeout específico para conversas/perguntas (modelos podem ser mais lentos)
OLLAMA_CONVERSATION_TIMEOUT_MS=120000
# Timeout para ações/comandos (geralmente mais rápido)
OLLAMA_ACTION_TIMEOUT_MS=60000

# Ollama Performance Configuration
# Número de threads para processamento (aumentar pode melhorar velocidade, mas usar mais CPU)
OLLAMA_NUM_THREADS=8

# Open Interpreter Timeout (ms)
# Timeout para execução de tarefas complexas com Open Interpreter
OPEN_INTERPRETER_TIMEOUT_MS=120000

# ElevenLabs TTS (Opcional)
ELEVENLABS_API_KEY=your_api_key_here
ELEVENLABS_VOICE_ID=your_voice_id_here

# OAuth (Opcional)
VITE_OAUTH_PORTAL_URL=https://oauth.manus.computer
VITE_APP_ID=your_app_id

# Database (Opcional)
DATABASE_URL=sqlite:./data.db

# WebSocket
WS_PORT=3000

# Resource Management
MAX_VRAM=16
IDLE_TIMEOUT=300000

# Tailscale Funnel (Opcional)
# Habilita o Tailscale Funnel automaticamente ao iniciar o servidor
# Requer Tailscale instalado e autenticado
USE_TAILSCALE_FUNNEL=false

# HTTPS (Opcional)
# Habilita HTTPS no servidor
# Requer certificados SSL em certs/cert.pem e certs/key.pem
# Ou configure SSL_CERT_PATH e SSL_KEY_PATH
USE_HTTPS=false
SSL_CERT_PATH=./certs/cert.pem
SSL_KEY_PATH=./certs/key.pem
